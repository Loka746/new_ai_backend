{
  "name": "vibecoding",
  "displayName": "VibeCoding",
  "description": "AI coding assistant with code generation",
  "version": "0.0.2",
  "publisher": "vibe",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "./out/extension.js",
  "activationEvents": [
    "onStartupFinished"
  ],
  "categories": [
    "Other",
    "Machine Learning"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "aiCodeSidebar",
          "title": "AI Code",
          "icon": "media/logo.png"
        }
      ]
    },
    "views": {
      "aiCodeSidebar": [
        {
          "id": "vibecoding.chatView",
          "name": "AI Chat",
          "type": "webview",
          "icon": "media/logo"
        }
      ]
    },
    "commands": [
      {
        "command": "vibecoding.chat",
        "title": "Open VibeCoding Chat"
      },
      {
        "command": "vibecoding.configureBackend",
        "title": "Configure Backend URL"
      },
      {
        "command": "vibecoding.checkBackend",
        "title": "Check Backend Connection"
      }
    ],
    "configuration": {
      "title": "VibeCoding",
      "properties": {
        "vibecoding.backendUrl": {
          "type": "string",
          "default": "https://vibe-vscodeextension-18.onrender.com",
          "description": "URL of the Render backend server"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.80.0",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "ai",
    "coding",
    "assistant"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vibe/vibecoding/issues"
  },
  "homepage": "https://github.com/vibe/vibecoding#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/vibe/vibecoding.git"
  },
  "files": [
    "out/**/*",
    "media/**/*",
    "python/**/*"
  ],
  "dependencies": {
    "@types/node-fetch": "^2.6.13",
    "node-fetch": "^3.3.2"
  }
}